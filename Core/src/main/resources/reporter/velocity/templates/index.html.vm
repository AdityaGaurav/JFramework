            font-weight: bold;
            padding: 7px 12px;
            text-align: center;
        }
        #gauge{
            text-align: center;
        }
        .chart-footer {
            border-top: 1px solid #cdcdcd;
            height: 50px;
            margin-bottom: 2px;
            margin-top: 10px;
        }
        .chart-footer .chart-footer-element {
            border-right: 1px solid #cdcdcd;
            float: left;
            height: 50px;
        }
        .chart-footer .chart-heading {
            font-size: 15px;
            margin-top: 4px;
            text-align: center;
        }
        .chart-footer .chart-desc {
            margin-top: 5px;
            text-align: center;
        }
        .chart-footer .no-border-right {
            border-right: medium none;
        }
        .icos_pie_chart {
            background-image: url("../img/icons/pie_chart.png");

    <script id="3d.pie.chart.js" type="text/javascript">

        google.load( "visualization", "1", { packages:[ "corechart" ] } );
        google.setOnLoadCallback( drawChart );
        function drawChart()
        {
            var data = google.visualization.arrayToDataTable
            (
                [
                    [ 'Summary', 'Suites Summary' ],
                    [ 'Passed', $scenario.getSuccessTestCasesInstancesCount() ],
                    [ 'Failed', $scenario.getFailedTestCaseInstanceCount() ],
                    [ 'Failed with Success Percentage', $scenario.getFailedWithSuccessPercentageTestCaseInstanceCount() ],
                    [ 'Skipped', $scenario.getSkippedTestCaseInstanceCount() ],
                    [ 'Ignored', $scenario.getIgnoredTestCaseCount() ]
                ]
            );

	        var options =
		        {
		            #if( $const.PIE_USE_BACKGROUND_COLOR )
	                    backgroundColor: "$const.PIE_BACKGROUND_COLOR",
			        #else
	                    backgroundColor : {
	                        stroke : " $const.PIE_BACKGROUND_COLOR_STROKE",
	                        strokeWidth : $const.PIE_BACKGROUND_COLOR_STROKE_WIDTH,
	                        fill : "$const.PIE_BACKGROUND_COLOR_FILL"
	                    },
		            #end
		            colors : [ '$const.PIE_COLOR_PASSED', '$const.PIE_COLOR_FAILED', '$const.PIE_COLOR_SUCCESS_PERCENTAGE', '$const.PIE_COLOR_SKIPPED', '$const.PIE_COLOR_IGNORED' ],
		            enableInteractivity : $const.PIE_ENABLE_INTERACTIVITY,
		            fontSize : $const.PIE_FONT_SIZE,
		            fontName : "$const.PIE_FONT_NAME",
		            height: $const.PIE_HEIGHT,
		            legend : {
			            alignment : "$const.PIE_LEGEND_ALIGNMENT",
			            position : '$const.PIE_LEGEND_POSITION',
			            textStyle : {
				            color : '$const.PIE_LEGEND_TEXT_STYLE_COLOR',
				            fontSize : $const.PIE_LEGEND_TEXT_STYLE_FONT_SIZE
			            }
		            },
		            pieSliceText : "$const.PIE_SLICE_TEXT",
		            pieSliceTextStyle : {
			            color : '$const.PIE_SLICE_TEXT_STYLE_COLOR',
			            fontSize : $const.PIE_SLICE_TEXT_FONT_SIZE },
		            pieStartAngle : $const.PIE_START_ANGLE,
		            tooltip : {
			            text : "$const.PIE_TOOLTIP_TEXT",
			            textStyle : {
				            color : '$const.PIE_TOOLTIP_TEXT_STYLE_COLOR',
				            fontSize : $const.PIE_TOOLTIP_TEXT_STYLE_FONT_SIZE,
				            bold : $const.PIE_TOOLTIP_TEXT_STYLE_FONT_BOLD
			            },
			            showColorCode : $const.PIE_TOOLTIP_SHOW_COLOR_CODE
		            },
		            is3D : $const.PIE_OPTIONS_IS3D
			        #if ( $const.PIE_OPTIONS_SHOW_TITLE )
				        title: $const.PIE_OPTIONS_TITLE
				    #end
	            };

            var chart = new google.visualization.PieChart( document.getElementById( 'pie.chart.3d' ) );
            chart.draw( data, options );
        }

    </script>
</head>
